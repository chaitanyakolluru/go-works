package main

import "testing"

type argStructure struct {
	nums   []int
	target int
}

type caseStructure struct {
	args argStructure
	want int
}

func TestThreeSumClosest(t *testing.T) {
	cases := map[string]caseStructure{"should give 2": caseStructure{args: argStructure{
		nums:   []int{-1, 2, 1, -4},
		target: 1}}}

		for name, case := range cases {
		t.Run(name,func(t *testing.T) {} )
	nums := []int{-1, 2, 1, -4}
	want := 1
	}
	got := threeSumClosest(nums, 1)
	if got != want {
		t.Errorf("got: %d, want: %d", got, want)
	}
}
